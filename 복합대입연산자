#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main(void)
{
	int x, y, z;
	printf("x = ");
	scanf("%d",&x);
	printf("y = ");
	scanf("%d",&y);
	printf("z = ");
	scanf("%d",&z);
	x += 1;
	y *= 2;
	z %= 10 + 20;
	printf("___________________________\nx = %d\ny = %d\nz = %d\n",x, y, z);

	return 0;
}
