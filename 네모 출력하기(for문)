#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main(void)
{
	int width, length;
	scanf("%d %d", &width, &length);
	for (int i = 0; i <= width; i++)
		printf("*");
	printf("\n");
	for (int i = 0; i <= length; i++)
	{
		printf("*");
		for (int j = 0; j < width - 1; j++)
			printf(" ");
		printf("*\n");
	}
	for (int i = 0; i <= width; i++)
		printf("*");
	return 0;
}
