#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
	int a = 0, sum = 0;
	int answer = rand() % 100;
	while (a != answer)
	{
		printf("정답을 추측하시오:");
		scanf("%d", &a);
		if (a > answer)
			printf("LOW\n");
		else if (a < answer)
			printf("HIGH\n");
		sum++;
	}
	printf("축하합니다. 시도횟수 = %d", sum);
	return 0;
}
