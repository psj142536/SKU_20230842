#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>

int prime(int n)
{
	int i = 1;
	while (1)
	{
		if (i == n)
			printf("%d는 소수가 아닙니다.", n);
		else if (n % i == 0)
		{
			printf("%d은 소수가 아닙니다.", n);
			break;
		}
	}	
}

int main()
{
	int n = 0;
	printf("정수를 입력하시오: ");
	scanf("%d", &n);
	prime(n);
	return 0;
}
